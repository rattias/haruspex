namespace haruspex.producer.serialization.flatbuf;

enum ContextType: byte {TRACE, ENTITY, BLOCK, POINT}

table Tag {
  key: string;
  value: string;
}

table TagContext {
  type: ContextType = TRACE;
  tags: [Tag];
}

table TraceEvent {
	seqNum: long;
	timestamp: long;
	traceId: long;
	entityId: long;
	blockId: int;
	tags: [TagContext];
}